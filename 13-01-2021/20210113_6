#include <stdio.h>


int main(){
    signed char a = -127;
    printf("%d\n", a);

    unsigned char b = 255;
    printf("%d\n", b);

    signed int c = 6237498;
    printf("%d\n", c);

    unsigned int d = 4294967292;
    printf("%lu\n", d);

    signed long long int e = -9000000000000775845;
    printf("%lli\n", e);

    return 0;
}
