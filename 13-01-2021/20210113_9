#include <stdio.h>


int main(){
    
    float a = 4.9876543;
    printf("%.7f\n", a);

    double b = 7.123456789012345678890;
    printf("%.21f\n", b);

    double c = 18398458438583853.28;
    printf("%.2f\n", c);

    long double d = 18398458438583853.39875937284928422;
    printf("%.17Lf\n", d);

    return 0;
}
