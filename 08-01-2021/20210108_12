12. Да се преобразуват от шестнадесетична в десетична бройна система числата:

а) 16F.AA
1*16^2 + 6*16^1 + 15*16^0 + 10*16^-1 + 10*16^-2
= 256 + 96 + 15 + 0.625 + 0.0390625 = 367.6640625

16F.AA(16) = 367.6640625(10)

б) 19.CA
1*16^1 + 9*16^0 + 12*16^-1 + 10*16^-2
= 25 + 0.75 + 0.0390625 = 25.7890625

19.CA(16) = 25.7890625(10)

в) 4A.CD
4*16^1 + 10*16^0 + 12*16^-1 + 13*16^-2
= 74 + 0.75 + 0.05078125 = 74.80078125

4A.CD(16) = 74.80078125(10)

г) 7F.08
7*16^1 + 15*16^0 + 0*16^-1 + 8*16^-2
= 127 + 0 + 0.03125 = 127.03125

7F.08(16) = 127.03125(10)
